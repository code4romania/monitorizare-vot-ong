{
  "project": {
    "version": "1.0.0-beta.21",
    "name": "monitorizare-vot"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "../api/MonitorizareVot.Ong/src/MonitorizareVot.Api/wwwroot",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "../node_modules/font-awesome/scss/font-awesome.scss",
